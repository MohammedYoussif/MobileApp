{
  "expo": {
    "name": "B.Expo",
    "slug": "BExpo",
    "splash": null,
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mIcon.png",
    "scheme": "bexpomobile",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exponfc.exponfc",
      "buildNumber": "22",
      "associatedDomains": [
        "applinks:bexpo.xyz",
        "applinks:dashboard.bexpo.xyz"
      ],
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      }
    },
    "android": {
      "versionCode": 4,
      "edgeToEdgeEnabled": true,
      "package": "com.exponfc.exponfc",
      "permissions": ["BILLING"],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "bexpo.xyz",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "dashboard.bexpo.xyz",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "extra": {
      "supportsRTL": true,
      "router": {},
      "eas": {
        "projectId": "7d7bd1c2-f042-4273-8144-4072d913dea7"
      }
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.314892553525-49lu03aeb8intjc7pq9fpjc414cgfvut"
        }
      ],
      [
        "react-native-iap",
        {
          "paymentProvider": "both"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow BExpo to access your camera"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow BExpo to access your camera",
          "microphonePermission": "Allow BExpo to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
