{
  "expo": {
    "name": "bExpoMobile",
    "slug": "bExpoMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bexpomobile",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exponfc.exponfc",
      "supportsRTL": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.exponfc.exponfc",
      "supportsRTL": true
    },
    "extra": {
      "supportsRTL": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
